<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="create_table_presentation" author="kulikoff">

        <createTable tableName="t_presentation">

            <column name="id" type="bigint" autoIncrement="true" incrementBy="1">
                <constraints primaryKey="true" nullable="false"/>
            </column>

            <column name="title" type="varchar(255)"/>

        </createTable>

    </changeSet>

    <changeSet  id="insert_table_presentation" author="kulikoff"  >
        <insert  schemaName="conference_test11"
                 tableName="t_presentation">
            <column  name="title"  value="Animals"/>
        </insert>
        <insert  schemaName="conference_test11"
                 tableName="t_presentation">
            <column  name="title"  value="Programming"/>
        </insert>
        <insert  schemaName="conference_test11"
                 tableName="t_presentation">
            <column  name="title"  value="Java Applications"/>
        </insert>
    </changeSet>



</databaseChangeLog>
